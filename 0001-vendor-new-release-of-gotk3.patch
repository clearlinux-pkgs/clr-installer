+
+	cstr := C.CString(resourcePath)
+	defer C.free(unsafe.Pointer(cstr))
+
+	cPreserveAspectRatio := gbool(preserveAspectRatio)
+
+	c := C.gdk_pixbuf_new_from_resource_at_scale(
